# Node Backend Configuration

# Server Port
PORT=5000

# Python LLM Service URL (change this if your Python service is on a different port)
# If Python is on localhost:8000, use: http://localhost:8000
# If Python is on localhost:5000, use: http://localhost:5000
# If deployed, use: https://your-python-backend.com
PYTHON_LLM_URL=http://localhost:8000

# Supabase Configuration
SUPABASE_URL=https://your-supabase-url.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Environment
NODE_ENV=development
